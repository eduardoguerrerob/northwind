{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run egb.proyNW3-uimodule",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/egbproyNW3/flpSandbox.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/uimodule",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/egb.proyNW3/uimodule\"}"
      },
      "envFile": "${workspaceFolder}/uimodule/.env1",
      "preLaunchTask": "Build egb.proyNW3"
    }
  ]
}